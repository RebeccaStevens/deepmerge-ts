{
  "name": "deepmerge-ts",
  "version": "0.0.0-development",
  "description": "Deeply merge 2 or more objects respecting type information.",
  "keywords": [
    "merge",
    "deepmerge",
    "deep merge",
    "deep-merge",
    "inferred types",
    "inferred-types",
    "recursive merge",
    "recursive-merge",
    "ts",
    "ts merge",
    "ts-merge",
    "typescript",
    "typescript merge",
    "typescript-merge"
  ],
  "homepage": "https://github.com/RebeccaStevens/deepmerge-ts#readme",
  "bugs": {
    "url": "https://github.com/RebeccaStevens/deepmerge-ts/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RebeccaStevens/deepmerge-ts"
  },
  "license": "BSD-3-Clause",
  "author": {
    "name": "Rebecca Stevens",
    "email": "rebecca.stevens@outlook.co.nz"
  },
  "sideEffects": false,
  "exports": {
    "import": "./dist/node/index.mjs",
    "require": "./dist/node/index.cjs",
    "default": "./dist/node/index.cjs"
  },
  "main": "dist/node/index.cjs",
  "module": "dist/node/index.mjs",
  "types": "dist/node/types/legacy/v4_0.d.ts",
  "typesVersions": {
    "<4.1": {
      "*": [
        "dist/node/types/legacy/v4_0.d.ts"
      ]
    },
    ">=4.1": {
      "*": [
        "dist/node/types/current/index.d.ts"
      ]
    }
  },
  "files": [
    "dist/node/",
    "package.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "prebenchmark": "yarn build && yarn link",
    "benchmark": "cd benchmark && yarn benchmark; cd ..",
    "prebuild": "rimraf build",
    "build": "yarn build:npm && yarn build:deno",
    "postbuild": "rimraf build",
    "pre:build:deno": "rimraf dist/deno",
    "build:deno": "tsc -p tsconfig.build.deno.json && denoify",
    "prebuild:npm": "rimraf dist/node types/current",
    "build:npm": "rollup -c",
    "check-format": "prettier --list-different \"./**/*.{md,ts,yml}\"",
    "check-spelling": "cspell --config=.cspell.json \"**/*.{md,ts}\"",
    "cz": "git-cz",
    "format": "prettier --write \"./**/*.{md,ts,yml}\"",
    "lint": "yarn lint:js && yarn lint:md",
    "lint:js": "eslint .",
    "lint:md": "markdownlint \"**/*.md\" --config=.markdownlint.json --ignore-path=.markdownlintignore",
    "prepare": "husky install",
    "test": "yarn test:js && yarn test:types",
    "test:js": "nyc ava",
    "test:types": "dtslint --localTs node_modules/typescript/lib --expectOnly tests/types"
  },
  "dependencies": {
    "is-plain-object": "^5.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@cspell/dict-cryptocurrencies": "^2.0.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@rebeccastevens/eslint-config": "^1.3.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.2",
    "@semantic-release/npm": "^9.0.0",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/node": "^17.0.16",
    "@types/rollup-plugin-auto-external": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "ava": "^4.0.1",
    "codecov": "^3.8.3",
    "commitizen": "^4.2.4",
    "cspell": "^5.18.4",
    "denoify": "^0.11.0",
    "dtslint": "^4.2.1",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-ava": "^13.2.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-eslint-plugin": "^4.1.0",
    "eslint-plugin-functional": "^4.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^37.8.0",
    "eslint-plugin-markdown": "^2.2.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-sonarjs": "^0.11.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "eslint-plugin-unicorn": "^40.1.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.3",
    "markdownlint-cli": "^0.31.1",
    "marked": "^4.0.12",
    "nyc": "^15.1.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.67.1",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-dts": "^4.1.0",
    "semantic-release": "^19.0.2",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.5.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "denoify": {
    "out": "dist/deno",
    "index": "src/index.ts",
    "ports": {
      "is-plain-object": "https://raw.githubusercontent.com/jonschlinkert/is-plain-object/v5.0.0/is-plain-object.js"
    }
  },
  "engines": {
    "node": ">=12.4.0"
  }
}
